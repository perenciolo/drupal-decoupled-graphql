{%
  set classes = [
    'paragraph',
    'paragraph--type--' ~ paragraph.bundle|clean_class,
    view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
    not paragraph.isPublished() ? 'paragraph--unpublished'
  ]
%}
{{ attach_library('multiple_carousel/default') }}
{% block paragraph %}
  <div{{attributes.addClass(classes)}} id="cr-wrapper-{{id}}" data-src='{{ data }}'></div>
{% endblock paragraph %}
